    "import math\n",
    "\n",
    "x1 = int(input(\"Enter x1: \"))\n",
    "x2 = int(input(\"Enter x2: \"))\n",
    "y1 = int(input(\"Enter y1: \"))\n",
    "y2 = int(input(\"Enter y2: \"))\n",
    "\n",
    "ans = math.sqrt(math.pow(x2 - x1, 2) + math.pow(y2 - y1, 2) * 1.0);\n",
    "\n",
    "print(\"Distance between two coordinates: \", round(ans, 2))"
   ]
